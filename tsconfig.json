{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "noEmitOnError": false,
    "noEmit": true,
    "rootDir": ".",
    "emitDecoratorMetadata": true,
    "suppressExcessPropertyErrors":true,
    "suppressImplicitAnyIndexErrors":true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "sourceRoot": "",
    "inlineSourceMap": true,
    "inlineSources": false
  },
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": false
}
